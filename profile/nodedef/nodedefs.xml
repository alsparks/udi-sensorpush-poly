<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
          <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>

    <nodeDef id="sensor" nls="sensor">
        <editors />
        <sts>
          <st id="ST" editor="bool" />
          <st id="GV1" editor="temp_f" />
          <st id="GV2" editor="humidity_r" />
          <st id="GV3" editor="battery" />
          <st id="GV4" editor="rssi" />
        </sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="QUERY" />
            </accepts>
        </cmds>
    </nodeDef>

</nodeDefs>
